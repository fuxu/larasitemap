{
    "name": "fuxu/larasitemap",
    "description": "A simple sitemap generator for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fu Xu",
            "email": "fuxu@fuxu.name"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "FuXu\\LaraSitemap\\": "src/"
        }
    },
    "extra": {
		"laravel": {
			"providers": [
				"FuXu\\LaraSitemap\\SitemapServiceProvider"
			]
		}
	}
}
